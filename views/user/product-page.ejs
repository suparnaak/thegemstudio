<!--header begins-->
<%- include("../../views/partials/user/header") %>
<!--header ends-->

<div class="breadcrumbs">
    <div class="container_12">
        <div class="grid_12">
            <a href="/">Home</a><span></span><a href="#">Products</a><span></span><span class="current"><%= product.name %></span>
        </div><!-- .grid_12 -->
    </div><!-- .container_12 -->
</div><!-- .breadcrumbs -->

<section id="main">
    <div class="container_12">
        <div id="content" class="grid_12">
            <header>
                <h1 class="page_title"><%= product.name %></h1>
            </header>
                    
            <article class="product_page negative-grid">
                <div class="grid_5 img_slid" id="products">
                    <img class="sale" src="/img/sale.png" alt="Sale">
                    <div class="preview slides_container">
                        <div class="prev_bg">
                            <a href="/uploads/products/<%= product.images[0] %>" class="jqzoom" rel='gal1' title="">
                                <img src="/uploads/products/<%= product.images[0] %>" alt="<%= product.name %>" title="" style="width: 100%">
                            </a>
                        </div>
                    </div><!-- .preview -->

                    <div class="next_prev">
                        <a id="img_prev" class="arows" href="#"><span>Prev</span></a>
                        <a id="img_next" class="arows" href="#"><span>Next</span></a>
                    </div><!-- .next_prev -->

                    <ul class="small_img clearfix" id="thumblist">
                        <% product.images.forEach((image, index) => { %>
                            <li>
                                <a class="zoomThumbActive" href='javascript:void(0);' rel="{gallery: 'gal1', smallimage: '/uploads/products/<%= image %>', largeimage: '/uploads/products/<%= image %>'}">
                                    <img src='/uploads/products/<%= image %>' alt="">
                                </a>
                            </li>
                        <% }) %>
                    </ul><!-- .small_img -->
                    <div id="pagination"></div>
                </div><!-- .grid_5 -->

                <div class="grid_7">
                    <div class="entry_content">
                        <p><%= product.description %></p>
                        
                        <div class="ava_price">
                            <div class="price">
                                <% if (product.discount > 0) { %>
                                    <div class="price_old">₹<%= product.price + product.discount %></div>
                                <% } %>
                                ₹<%= product.price %>
                            </div><!-- .price -->

                            <div class="availability_sku">
                                <div class="availability">
                                    Availability: <span><%= product.status %></span>
                                </div>
                                <div class="sku">
                                    SKU: <span><%= product.sku || 'Not Available' %></span>
                                </div>
                                <div class="parameter_selection">
                            
                            
                                Material: <span><%= product.material %></span>
                            
                        </div><!-- .parameter_selection -->
                            </div><!-- .availability_sku -->
                            <div class="clear"></div>
                        </div><!-- .ava_price -->

                        

                        <div class="cart">
                            <a href="#" class="bay"><img src="/img/bg_cart.png" alt="Buy" title="">Add to Cart</a>
                            <a href="#" class="wishlist"><span></span>Add to Wishlist</a>
                            <a href="#" class="compare"><span></span>Add to Compare</a>
                        </div><!-- .cart -->
                    </div><!-- .entry_content -->
                </div><!-- .grid_7 -->
                <div class="clear"></div>

